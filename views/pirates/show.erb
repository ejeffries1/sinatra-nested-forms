<h1>Display your Pirate here</h1>
    <div class="pirates">
        <h1><%=@pirate.name%></h1>
        <h1><%=@pirate.weight%></h1>
        <h1><%=@pirate.height%></h1>
        </div>
<h2>Display your first ship here</h2>
    <% @ships.each do |ship| %>
        <p>Name: <%= ship.name %></p>
        <p>Type: <%=ship.type%></p>
        <p>Booty: <%=ship.booty%></p>
    <% end %>
<h2>Display your second ship here</h2>
    <%@ships.each do |ship|%>
        <p>Name: <%=ship.name%></p>
        <p>Type: <%=ship.type%></p>
        <p>Booty: <%=ship.booty%></p>
    <% end %>
